{
  "name": "rtm-fe",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "api:generate": "npx openapi-generator-cli generate   -i http://localhost:8088/api/v1/v3/api-docs   -g typescript-axios   -o src/openapi   --skip-validate-spec   --api-package apis   --model-package models   --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,modelPropertyNaming=original"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@vee-validate/zod": "^4.15.1",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "pinia": "^3.0.1",
    "vee-validate": "^4.15.1",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.8.6",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
